#include <iostream>
#include<algorithm>
using namespace std;
 
int main() {
    int t; cin>>t;
    while(t--){
        int n;cin>>n;
        int c[n];
        for(int i =0;i<n;i++){
            cin>>c[i];
        }
        sort(c,c+n);
        bool x=true;
        for(int i =1;i<n-1;i+=2){
            if(c[i]==c[i+1]){
                continue;}
            else{x=false;
                 break;}
        }if(x)cout<<"Yes\n";
        else{cout<<"No\n";}
    }
    
    return 0;
}
